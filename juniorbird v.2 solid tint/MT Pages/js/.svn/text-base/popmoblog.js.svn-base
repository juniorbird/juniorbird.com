function showoriginal (photo,orientation)
	{
	switch (orientation)
		{
		case 'portrait':
			window.open(photo,'moblog','width=480,height=640');
			return;
		case 'landscape':
			window.open(photo,'moblog','width=640,height=480');
			return;
		default:
			window.open(photo,'moblog','width=640,height=640');
			return;
		}
	}